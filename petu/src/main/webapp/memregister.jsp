<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta http-equiv="pragma" content="no-cache">
<meta http-equiv="cache-control" content="no-cache">
<meta http-equiv="expires" content="0">
<script src="//cdn.bootcss.com/jquery/1.11.3/jquery.min.js"></script>

<title>注册</title>
</head>
<body>
	<h2>注册</h2>
	<form method="post" action="MemRegisterAction.do">
		<table>
			<tr>
				<td>邮箱： <input name="admin.nickname" /></td>
			</tr>
			<tr>
				<td>性别： <input name="admin.sex" /></td>
			</tr>
			<tr>
				<td>地址： <input name="admin.address" /></td>
			</tr>

			<tr>
				<td>电话： <input name="admin.phone" /></td>
			</tr>
			
			<tr>
				<td>教育： <input name="admin.education" /></td>
			</tr>
			<tr>
				<td>薪水： <input name="admin.salary" /></td>
			</tr>
			<tr>
				<td>公司： <input name="admin.company" /></td>
			</tr>
			<tr>
				<td>婚姻： <input name="admin.marriage" /></td>
			</tr>
			<tr>
				<td>经验： <input name="admin.experience" /></td>
			</tr>
			
			<tr>
				<td>数量： <input name="admin.catcount" /></td>
			</tr>
			
			<tr>
				<td>原因： <input name="admin.buyreason" /></td>
			</tr>
			
			<tr>
				<td>自我介绍： <input name="admin.selfintroduce" /></td>
			</tr>
			
			<tr>
				<td><input type="submit" value="登陆"></td>
			</tr>

      
      </table>

	</form>
</body>
</html>





